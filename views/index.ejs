    <style>
        #my-ar-container {
            height: 600px;
            overflow: hidden;
        }
        
        #ready {
            top: 50%;
            right: 0;
            left: 0;
            width: 200px;
            margin: 0 auto 0 auto;
            visibility: hidden;
        }
        
        .Flex-color-Special {
            text-shadow: 0 0 10px rgb(255, 0, 0, 0), 0 0 20px rgb(255, 0, 0, 0), 0 0 30px rgb(240, 76, 35), 0 0 40px #ffffff, 0 0 70px #ffffff;
            color: rgb(240, 249, 169);
            font-weight: 900;
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%);
            display: none;
        }
        
        @keyframes fadeIn {
            0% {
                opacity: 0;
            }
            100% {
                opacity: 1;
            }
            0% {
                opacity: 0;
            }
        }
        
        .Flex-fadeIn {
            animation-name: fadeIn;
            animation-duration: 2s;
            animation-iteration-count: infinite;
            animation-delay: 0s;
        }
    </style>
</head>

